    <style name="AddUnconfigured.Medium" parent="@style/AddUnconfigured">
        <item name="android:layout_marginRight">@dimen/add_icon_right_margin_medium</item>
        <item name="android:layout_marginBottom">@dimen/add_icon_bottom_margin_medium</item>
        <item name="android:src">@blur.res:drawable/widget_add_medium</item>
    </style>
    <style name="AddUnconfigured.Large" parent="@style/AddUnconfigured">
        <item name="android:layout_marginRight">@dimen/add_icon_right_margin_large</item>
        <item name="android:layout_marginBottom">@dimen/add_icon_bottom_margin_large</item>
        <item name="android:src">@blur.res:drawable/widget_add_large</item>
    </style>
    <style name="Karat">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="Karat.H" parent="@style/Karat">
        <item name="android:paddingTop">@dimen/karat_h_top</item>
        <item name="android:layout_marginLeft">@dimen/karat_h_left</item>
