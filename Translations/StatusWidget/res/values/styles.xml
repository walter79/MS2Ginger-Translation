    <style name="Content.Horizontal" parent="@style/Content">
        <item name="android:paddingTop">@dimen/content_pad_top</item>
        <item name="android:paddingRight">@dimen/content_pad_right</item>
        <item name="android:paddingBottom">@dimen/content_pad_bottom</item>
