        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:maxHeight">@dimen/edit_status_status_height</item>
    </style>
    <style name="TxtDateTimeStamp">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@blur.res:color/white</item>
    </style>
    <style name="CountIcon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">@blur.res:dimen/listitem_photo_hspacing</item>
    </style>
</resources>