    <style name="EditIcon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="EditIcon.Small" parent="@style/EditIcon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/ic_control_edit_status_small</item>
    </style>
    <style name="EmptyMessage" parent="@blur.res:style/Widget.Text.Body">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="EmptyMessage.Vertical" parent="@style/EmptyMessage">
        <item name="android:gravity">center</item>
    </style>
    <style name="EmptyMessage.Horizontal" parent="@style/EmptyMessage">
        <item name="android:gravity">center</item>
    </style>
    <style name="EmptyMessage.Vertical.Add" parent="@style/EmptyMessage.Vertical">
        <item name="android:layout_marginTop">-40.0dip</item>
    </style>
    <style name="EmptyMessage.Horizontal.Add" parent="@style/EmptyMessage.Horizontal">
        <item name="android:layout_marginTop">-24.0dip</item>
    </style>
