    <style name="Karat.V" parent="@style/Karat">
        <item name="android:paddingLeft">@dimen/karat_v_left</item>
        <item name="android:layout_marginTop">@dimen/karat_v_top</item>
        <item name="android:src">@drawable/karat_v</item>
