        <item name="android:layout_alignParentBottom">true</item>
    </style>
    <style name="Timestamp" parent="@blur.res:style/Widget.Text.Timestamp">
        <item name="android:layout_marginRight">@dimen/time_padding_right</item>
        <item name="android:includeFontPadding">true</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>
    <style name="HOME_CommentCount">
        <item name="android:textSize">10.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@blur.res:color/black</item>
