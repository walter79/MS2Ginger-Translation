    </style>
    <style name="Sleeve.Image" parent="@style/Sleeve">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:scaleType">centerCrop</item>
    </style>
    <style name="Sleeve.Image.Vertical" parent="@style/Sleeve.Image">
        <item name="android:paddingLeft">@dimen/vertical_img_pad_left</item>
        <item name="android:paddingTop">@dimen/vertical_img_pad_top</item>
        <item name="android:paddingRight">@dimen/vertical_img_pad_right</item>
