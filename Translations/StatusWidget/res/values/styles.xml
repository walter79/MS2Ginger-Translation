        <item name="android:paddingRight">@dimen/horizontal_img_pad_right</item>
        <item name="android:paddingBottom">@dimen/horizontal_img_pad_bottom</item>
    </style>
    <style name="Content">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="Content.Vertical" parent="@style/Content">
        <item name="android:paddingLeft">@dimen/content_pad_left</item>
        <item name="android:paddingRight">@dimen/content_pad_right</item>
