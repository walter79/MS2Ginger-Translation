        <item name="android:paddingLeft">@dimen/content_pad_left</item>
        <item name="android:paddingRight">@dimen/content_pad_right</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="BlurMessage.Horizontal" parent="@style/BlurMessage">
