<?xml version="1.0" encoding="UTF-8"?>
<resources>
    <style name="TextAppearance" parent="@android:style/TextAppearance" />
    <style name="TextAppearance.EditEvent_Label" parent="@style/TextAppearance">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="TextAppearance_ViewEvent_TitleNormal">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/black</item>
    </style>
    <style name="TextAppearance_ViewEvent_TitleCompleted">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/black</item>
    </style>
    <style name="TextAppearance_ViewEvent_TitleOverdue">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ff0f75b8</item>
    </style>
    <style name="TextAppearance_ViewEvent_ItemLabel">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="TextAppearance_ViewEvent_Item">
        <item name="android:textSize">18.0sp</item>
    </style>
    <style name="Background_ViewEvent_Title">
        <item name="android:background">#ffdddddd</item>
    </style>
    <style name="MinusButton">
        <item name="android:background">@blur.res:drawable/zz_moto_btn_remove_selector</item>
    </style>
    <style name="PlusButton">
        <item name="android:background">@blur.res:drawable/zz_moto_btn_add_selector</item>
    </style>
    <style name="TextAppearance.TagDialog_item" parent="@style/TextAppearance">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="TextAppearance_ListItem_TitleNormal">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/black</item>
    </style>
    <style name="TextAppearance_ListItem_TitleCompleted">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/black</item>
    </style>
    <style name="TextAppearance_ListItem_TitleOverdue">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ff0f75b8</item>
    </style>
    <style name="TasksTheme.ActiveNoTitle" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="TasksTheme.Normal" parent="@android:style/Theme.Light">
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="Theme.SelectSearchSource" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@style/Animation.SelectSearchSource</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowSoftInputMode">stateUnchanged|adjustResize</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
    <style name="Animation.SelectSearchSource" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/corpus_selector_open</item>
        <item name="android:windowExitAnimation">@anim/corpus_selector_close</item>
    </style>
    <style name="Btn.TitleBar" parent="@blur.res:style/Blur.Btn.TitleBar">
        <item name="android:background">@android:drawable/list_selector_background</item>
    </style>
    <style name="Theme.TagSelectSearchSource" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@style/Animation.TagSelectSearchSource</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowSoftInputMode">stateUnchanged|adjustResize</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
    <style name="Animation.TagSelectSearchSource" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/tag_selector_open</item>
        <item name="android:windowExitAnimation">@anim/tag_selector_close</item>
    </style>
    <style name="textxsmall">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:shadowColor">#ffffffff</item>
        <item name="android:shadowDx">2.0</item>
        <item name="android:shadowDy">2.0</item>
        <item name="android:shadowRadius">4.0</item>
    </style>
    <style name="textxsmall.Bold" parent="@style/textxsmall">
        <item name="android:textStyle">bold</item>
        <item name="android:shadowColor">#ffffffff</item>
        <item name="android:shadowRadius">4.0</item>
    </style>
</resources>