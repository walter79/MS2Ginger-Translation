        <item name="android:paddingTop">10.0sp</item>
        <item name="android:paddingBottom">10.0sp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@string/show_data_by_activity_link</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>
    <style name="DataUsageTitleText">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/text_color_primary</item>
        <item name="android:paddingTop">10.0sp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10.0dip</item>
        <item name="android:text">@string/usage_details_by_app_title</item>
        <item name="android:layout_alignParentLeft">true</item>
    </style>
    <style name="LastSevenDaysText">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/text_color_primary</item>
        <item name="android:paddingBottom">20.0sp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10.0dip</item>
        <item name="android:text">@string/last_seven_days</item>
        <item name="android:layout_below">@id/data_usage_title</item>
    </style>
    <style name="AppImage">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10.0dip</item>
        <item name="android:layout_marginTop">5.0dip</item>
        <item name="android:adjustViewBounds">true</item>
