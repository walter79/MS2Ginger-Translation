<?xml version="1.0" encoding="UTF-8"?>
<resources>
    <style name="DataMeter">
        <item name="android:background">@drawable/meter_data_bkg</item>
        <item name="android:padding">2.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">10.0dip</item>
        <item name="android:layout_marginRight">10.0dip</item>
        <item name="android:maxHeight">22.0dip</item>
        <item name="android:max">100</item>
        <item name="android:progress">50</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:minHeight">22.0dip</item>
    </style>
    <style name="DataMeter.Green" parent="@style/DataMeter">
        <item name="android:progressDrawable">@drawable/meter_data_green</item>
    </style>
    <style name="DataMeter.Yellow" parent="@style/DataMeter">
        <item name="android:progressDrawable">@drawable/meter_data_yellow</item>
    </style>
    <style name="DataMeter.Red" parent="@style/DataMeter">
        <item name="android:progressDrawable">@drawable/meter_data_red</item>
    </style>
    <style name="DataUsedTodayImage">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10.0dip</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:maxWidth">24.0sp</item>
        <item name="android:maxHeight">24.0sp</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_alignParentTop">true</item>
    </style>
    <style name="DataUsedTodayText">
        <item name="android:textColor">@color/text_color_primary</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10.0dip</item>
        <item name="android:text">@string/data_used_today_label</item>
    </style>
    <style name="BarGraph">
        <item name="android:background">@drawable/grid_data</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="DataUsageText">
